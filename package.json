{
  "name": "hospital-appointment-system",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "generate-mongodb-schemas": "bash hascli.sh generate-schemas",
    "build": "npm run prisma:generate  && npm run generate-mongodb-schemas && nest build && npm run move:assets:public",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "seed:run": "ts-node -r tsconfig-paths/register ./libs/common/src/database/seeds/run.seed.ts",
    "prisma:generate": "npx prisma generate --schema prisma/schema.prisma",
    "start:dev": "npm run prisma:generate && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "moveAssets": "cp -r ./apps/notification-service/src/core/templates ./dist/apps/notification-service/src/core/templates",
    "movePublic": "cp -r ./apps/notification-service/src/core/public ./dist/apps/notification-service/src/core/public",
    "move:assets:public": "npm run moveAssets && npm run movePublic"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^5.4.1",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^7.4.0",
    "@types/amqplib": "^0.10.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/hbs": "^4.0.4",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.7",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "@zxcvbn-ts/language-en": "^3.0.2",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.4",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cli-color": "^2.0.4",
    "commander": "^12.1.0",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.6",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.2.0",
    "express-session": "^1.18.0",
    "glob": "^11.0.0",
    "handlebars": "^4.7.8",
    "hbs": "^4.2.0",
    "helmet": "^7.1.0",
    "inquirer": "^10.2.2",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.6.2",
    "nanoid": "^3.3.7",
    "nest-winston": "^1.9.7",
    "reflect-metadata": "^0.2.2",
    "resend": "^4.0.0",
    "rxjs": "^7.8.1",
    "winston": "^3.14.2",
    "winston-transport": "^4.8.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.1",
    "@prisma/client": "^5.21.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cli-color": "^2.0.6",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.16.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.21.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@common/common(|/.*)$": "<rootDir>/libs/common/src/$1"
    }
  }
}
